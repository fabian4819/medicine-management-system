{
  "version": 2,
  "builds": [
    {
      "src": "server/app.js",
      "use": "@vercel/node",
      "config": {
        "maxLambdaSize": "10mb"
      }
    }
  ],
  "routes": [
    {
      "src": "/api/v1/(.*)",
      "dest": "/server/app.js"
    },
    {
      "src": "/api/(.*)",
      "dest": "/server/app.js"
    },
    {
      "src": "/test",
      "dest": "/server/app.js"
    },
    {
      "src": "/(.*\\.(js|css|html|png|jpg|jpeg|gif|svg|ico|woff|woff2|ttf|eot))",
      "dest": "/public/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/public/index.html"
    }
  ],
  "env": {
    "NODE_ENV": "production",
    "APP_ENV": "production",
    "USE_AIVEN": "true"
  },
  "regions": ["sin1"],
  "framework": null,
  "functions": {
    "server/app.js": {
      "maxDuration": 10
    }
  }
}